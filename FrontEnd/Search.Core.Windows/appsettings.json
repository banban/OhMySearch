{
  "AppSettings": {
    "SiteTitle": "Oh My Search",
    "Author": "Andrew Butenko",
    "Wiki": "https://github.com/banban/OhMySearch/wiki",
    "Support": "https://github.com/banban/OhMySearch/issues",
    "ListOfValues": [ "Value1", "Value2" ],
    "Redirects": [
      {
        "Name": "Code",
        "Title": "Code Search on Visual Studio Online",
        "Url": "https://{myaccount}.visualstudio.com/IT/_search?type=Code_a=search&_a=search&text="
      },
      {
        "Name": "Reports",
        "Title": "SSRS Reports Search",
        "Url": "http://{myaccount}/Reports/Pages/SearchResults.aspx?SearchText="
      },
      {
        "Name": "SPOL",
        "Title": "SharePoint Online Search",
        "Url": "https://{myaccount}.sharepoint.com/_layouts/15/sharepoint.aspx?v=search&q="
      },
      {
        "Name": "Intranet",
        "Title": "Intranet Search",
        "Url": "https://intranet/search/Pages/Results.aspx?s=All%20Sites&k="
      }
    ]
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "Data": {
    "ElasticSearch": {
      "Url": "http://localhost:9200",
      "User": "",
      "Password": ""
    },
    "ACL": {
      "Url": "http://localhost:49351/api/Auth/GetACL?upn={0}&path={1}"
    },
    "ImageMagic": {
      "HomePath": "C:\\Program Files\\ImageMagick-7.0.3-Q16"
    }
  }
}
